<div id="detailsMovie">
    <h1>Details</h1>
    <div class="movie">
        <h2>Title: {{title}} ({{year}})</h2>
        <img
            src="{{image}}">
        <p>{{description}}</p>
        <h2>Genres</h2>
        <ul class="genres">
            <li>{{genres}}</li>
        </ul>
        <p>Available Tickets: {{ticketsCount}}</p>
        <button><a href="#/buyTicket/{{objectId}}">Buy Ticket</a></button>
    </div>
</div>